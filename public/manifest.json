{
    "manifest_version":3,
    "version": "1.0.0",
    "name": "tracking",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/",
        "https://*/"
    ],
    "background": {
        "service_worker": "services/background.js"
    }
}